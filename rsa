import sys
import math

def is_prime(num):
    if num < 2:
        return False
    if num == 2:
        return True
    if num % 2 == 0:
        return False
    for i in range(3, int(math.sqrt(num)) + 1, 2):
        if num % i == 0:
            return False
    return True

def factorize_rsa(n):
    for p in range(2, n):
        if is_prime(p) and n % p == 0:
            q = n // p
            if is_prime(q):
                return p, q
    return None, None

def main():
    if len(sys.argv) != 2:
        print("Usage: rsa <file>")
        sys.exit(1)

    input_file = sys.argv[1]

    try:
        with open(input_file, 'r') as file:
            for line in file:
                n = int(line)
                p, q = factorize_rsa(n)
                if p is not None and q is not None:
                    print(f"{n}={p}*{q}")
                else:
                    print(f"Unable to factorize {n}")
    except FileNotFoundError:
        print("Input file not found.")

if __name__ == "__main__":
    main()
